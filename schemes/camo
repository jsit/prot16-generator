# Colour values
dbg=22372B # dark background
dhl=32473B # dark highlight
dt2=887d5c # dark bg text secondary
dt1=91a3a4 # dark bg text primary
lbg=e9eeeb # light background
lhl=d9dedb # light highlight
lt2=7695a8 # light bg text secondary
lt1=5a644e # light bg text primary
red=bd720d # red variant
ora=bd8c2b # orange variant
yel=baa60b # yellow variant
gre=589424 # green variant
cya=31a2ab # cyan variant
blu=528abd # blue variant
vio=8b7cba # violet variant
mag=b2749a # magenta variant

# Visual order used for colour mapping
p1=$gre # green
p2=$red # red
p3=$ora # orange
s1=$mag # magenta
s2=$vio # violet
t1=$blu # blue
t2=$cya # cyan
t3=$yel # yellow

# Terminal values follow this sequence
# 0=dbg; 1=red; 2=gre; 3=yel; 4=blu; 5=mag; 6=cya; 7=lhl;
# 8=dhl; 9=ora; 10=lt1; 11=dt2; 12=lt2; 13=vio; 14=dt1; 15=lbg.

# Terminal accent colours corresponding to the visual order
cp1=2
cp2=1
cp3=9
cs1=5
cs2=13
ct1=4
ct2=6
ct3=3

# Terminal colour codes
# corresponding to the palette
# followed by termcol description
cdbg=0      # black
cred=1      # red
cgre=2      # green
cyel=3      # yellow
cblu=4      # blue
cmag=5      # magenta
ccya=6      # cyan
clhl=7      # white
cdhl=8      # brblack
cora=9      # brred
clt1=10     # brgreen
cdt2=11     # bryellow
clt2=12     # brblue
cvio=13     # brmagenta
cdt1=14     # brcyan
clbg=15     # brwhite
