# Colour values
dbg=182d2a # dark background
dhl=283d3a # dark highlight
dt2=687d7a # dark bg text secondary
dt1=98adaa # dark bg text primary
lbg=fafaec # light background
lhl=eaeadc # light highlight
lt2=8a8a7c # light bg text secondary
lt1=5a5a4c # light bg text primary
red=c6414d # red variant
ora=c9753d # orange variant
yel=c6a13d # yellow variant
gre=679945 # green variant
cya=57ab8b # cyan variant
blu=378bbd # blue variant
vio=9a7bc8 # violet variant
mag=c97885 # magenta variant

# Visual order used for colour mapping
p1=$blu # blue
p2=$gre # green
p3=$vio # violet
s1=$cya # cyan
s2=$mag # magenta
t1=$ora # orange
t2=$yel # yellow
t3=$red # red

# Terminal values follow this sequence
# 0=dbg; 1=red; 2=gre; 3=yel; 4=blu; 5=mag; 6=cya; 7=lhl;
# 8=dhl; 9=ora; 10=lt1; 11=dt2; 12=lt2; 13=vio; 14=dt1; 15=lbg.

# Terminal accent colours corresponding to the visual order
cp1=4
cp2=2
cp3=13
cs1=6
cs2=5
ct1=9
ct2=3
ct3=1

# Terminal colour codes
# corresponding to the palette
# followed by termcol description
cdbg=0      # black
cred=1      # red
cgre=2      # green
cyel=3      # yellow
cblu=4      # blue
cmag=5      # magenta
ccya=6      # cyan
clhl=7      # white
cdhl=8      # brblack
cora=9      # brred
clt1=10     # brgreen
cdt2=11     # bryellow
clt2=12     # brblue
cvio=13     # brmagenta
cdt1=14     # brcyan
clbg=15     # brwhite
