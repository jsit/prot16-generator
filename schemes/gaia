# Colour values
dbg=2d2e2f # dark background
dhl=3d3e3f # dark highlight
dt2=786c4d # dark bg text secondary
dt1=94ae9f # dark bg text primary
lbg=ebeddf # light background
lhl=dbddcf # light highlight
lt2=7b8d9d # light bg text secondary
lt1=5B6c4a # light bg text primary
red=a67336 # red variant
ora=a9880b # orange variant
yel=ad9f4d # yellow variant
gre=7c8e2d # green variant
cya=5d9d7c # cyan variant
blu=6c9cba # blue variant
vio=8a8db7 # violet variant
mag=bd9198 # magenta variant

# Visual order used for colour mapping
p1=$red # red
p2=$gre # green
p3=$ora # orange
s1=$cya # cyan
s2=$yel # yellow
t1=$blu # blue
t2=$vio # violet
t3=$mag # magenta

# Terminal values follow this sequence
# 0=dbg; 1=red; 2=gre; 3=yel; 4=blu; 5=mag; 6=cya; 7=lhl;
# 8=dhl; 9=ora; 10=lt1; 11=dt2; 12=lt2; 13=vio; 14=dt1; 15=lbg.

# Terminal accent colours corresponding to the visual order
cp1=1
cp2=2
cp3=9
cs1=6
cs2=3
ct1=4
ct2=13
ct3=5

# Terminal colour codes
# corresponding to the palette
# followed by termcol description
cdbg=0      # black
cred=1      # red
cgre=2      # green
cyel=3      # yellow
cblu=4      # blue
cmag=5      # magenta
ccya=6      # cyan
clhl=7      # white
cdhl=8      # brblack
cora=9      # brred
clt1=10     # brgreen
cdt2=11     # bryellow
clt2=12     # brblue
cvio=13     # brmagenta
cdt1=14     # brcyan
clbg=15     # brwhite
